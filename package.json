{
  "name": "react-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite serve --port 8080",
    "build": "vite build",
    "preview": "vite preview --port 8080",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check:lint": "eslint \"**/*.{ts,tsx}\"",
    "check:ts": "tsc --noEmit",
    "check:ts:lint": "npm run check:ts && npm run check:lint",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{json,html,md,yaml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "npm run check:ts:lint",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@tool-pack/basic": "^0.0.20",
    "@types/jest": "^29.5.1",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react-scripts": "5.0.1",
    "sass": "^1.62.1",
    "typescript": "^4.9.5",
    "vite": "^4.3.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "react template starter",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/react-starter.git"
  },
  "author": "DYH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/react-starter/issues"
  }
}
